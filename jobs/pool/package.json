{
  "name": "@sushiswap/pool-job",
  "version": "0.0.0",
  "private": true,
  "description": "Pool jobs",
  "keywords": [
    "pool",
    "job"
  ],
  "homepage": "https://www.sushi.com",
  "repository": {
    "type": "git",
    "url": "https://github.com/sushiswap/sushiswap.git",
    "directory": "jobs/pools"
  },
  "license": "MIT",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "source": "src/index.ts",
  "typings": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "type": "module",
  "scripts": {
    "check": "tsc --pretty --noEmit",
    "clean": "rimraf .turbo && rimraf node_modules && rimraf dist",
    "build": "graphclient build --fileType js && rm -f .graphclient/package.json",
    "incentives": "ts-node --swc src/incentives.ts",
    "lint": "TIMING=1 eslint src --fix",
    "pools": "ts-node --swc src/pools.ts",
    "start": "ts-node src/server.ts",
    "server": "ts-node src/server.ts",
    "test": "jest --passWithNoTests",
    "validate": "graphclient validate",
    "volume": "ts-node --swc src/volume.ts"
  },
  "jest": {
    "preset": "@sushiswap/jest-config/node"
  },
  "dependencies": {
    "@ethersproject/address": "5.7.0",
    "@ethersproject/bignumber": "5.7.0",
    "@graphprotocol/client-add-source-name": "1.0.18",
    "@graphprotocol/client-auto-pagination": "1.1.16",
    "@graphprotocol/client-auto-type-merging": "1.0.23",
    "@graphprotocol/client-cli": "2.2.19",
    "@graphql-mesh/http": "0.3.27",
    "@graphql-mesh/transform-filter-schema": "0.15.24",
    "@graphql-mesh/transform-prefix": "0.12.22",
    "@graphql-mesh/transform-type-merging": "0.5.20",
    "@graphql-mesh/cache-localforage": "0.7.20",
    "@graphql-tools/merge": "8.4.0",
    "@graphql-mesh/utils": "0.43.21",
    "@graphql-mesh/merger-stitching": "0.18.23",
    "@graphql-mesh/graphql": "0.34.14",
    "@graphql-mesh/runtime": "0.46.22",
    "@graphql-mesh/cross-helpers": "0.3.4",
    "@graphql-mesh/store": "0.9.20",
    "@sushiswap/bentobox": "workspace:*",
    "@sushiswap/tines": "workspace:*",
    "@sushiswap/abi": "workspace:*",
    "@sushiswap/chain": "workspace:*",
    "@sushiswap/currency": "workspace:*",
    "@sushiswap/database": "workspace:*",
    "@sushiswap/graph-config": "workspace:*",
    "@sushiswap/validate": "workspace:*",
    "@sushiswap/wagmi-config": "workspace:*",
    "@wagmi/core": "0.10.10",
    "@whatwg-node/fetch": "0.8.4",
    "connect-timeout": "1.9.0",
    "date-fns": "2.29.3",
    "ethers": "5.7.2",
    "express": "4.18.2",
    "graphql": "16.6.0",
    "lodash.zip": "4.2.0"
  },
  "devDependencies": {
    "@sushiswap/eslint-config": "workspace:*",
    "@sushiswap/jest-config": "workspace:*",
    "@sushiswap/prettier-config": "workspace:*",
    "@sushiswap/typescript-config": "workspace:*",
    "@swc/core": "1.3.26",
    "@swc/helpers": "0.4.14",
    "@types/connect-timeout": "0.0.36",
    "@types/express": "4.17.15",
    "@types/lodash.zip": "4.2.7",
    "dotenv": "16.0.3",
    "eslint": "8.29.0",
    "jest": "29.3.1",
    "prettier": "2.8.1",
    "ts-node": "10.9.1",
    "typescript": "4.9.4"
